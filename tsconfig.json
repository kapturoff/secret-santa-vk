{
  "exclude": [
    "./tests/",
    "./node_modules/",
    "./dist/",
    "./src/bot/_bot.ts"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "dist",
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "src",
    "paths": {
      "interfaces": [
        "interfaces"
      ]
    },
    "esModuleInterop": true
  }
}